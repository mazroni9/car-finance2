{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_APP_URL": "https://car-finance2.vercel.app"
  },
  "git": {
    "deploymentEnabled": {
      "master": true
    }
  },
  "github": {
    "enabled": true,
    "autoAlias": true
  }
} 